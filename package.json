{
    "name": "TwitchSongRequests",
    "version": "1.0.0",
    "scripts": {
        "start-db": "./start-dynamo.sh",
        "build-server": "cd ./server && npm i",
        "build-ui": "cd ./ui && npm i",
        "start-server": "cd ./server && npm run start-local",
        "start-ui": "cd ./ui && npm run start-local",
        "start-local": "npm run start-db && (npm run build-server & npm run build-ui)  && (npm run start-server & npm run start-ui)"
    }
}